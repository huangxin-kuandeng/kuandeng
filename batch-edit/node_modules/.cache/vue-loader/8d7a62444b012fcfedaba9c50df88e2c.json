AC,CAAC;EAClB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC;AACf;AACA,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;EACjB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;AAC1B;AACA,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;EACjB,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC;AACb;AACA,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;EACb,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC;EACX,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC;AACnB;AACA,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;EACvB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC;AACnB;AACA,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;EACxB,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC;AACd","file":"D:/web_editor_hd/src/main/webapp/batch-edit/src/components/search.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"search\">\n    <el-select\n      v-model=\"value\"\n      placeholder=\"请选择\"\n      filterable\n      :filter-method=\"dataFilter\"\n      @change=\"changeTask\"\n      no-data-text=\"无任务\"\n      no-match-text=\"无匹配任务\"\n    >\n      <el-option\n        v-for=\"item in searchTasks\"\n        :key=\"renderValue(item)\"\n        :label=\"renderLabel(item)\"\n        :value=\"renderValue(item)\"\n        :title=\"renderLabel(item)\"\n      >\n        <span v-html=\"renderLabel(item)\"></span>\n        <span\n          style=\"float: right; color: #8492a6; font-size: 13px\"\n          v-html=\"renderValue(item)\"\n        ></span>\n      </el-option>\n    </el-select>\n    <el-button\n      class=\"btn-cliam\"\n      type=\"primary\"\n      icon=\"el-icon-search\"\n      @click=\"cliam()\"\n      >领取</el-button\n    >\n    <el-button\n      class=\"btn-logout\"\n      type=\"primary\"\n      icon=\"el-icon-search\"\n      @click=\"logout()\"\n      >退出</el-button\n    >\n  </div>\n</template>\n\n<script>\nimport * as sysApi from \"../services/sys\";\nimport * as userApi from \"../services/jquery.oauth2-client\";\nexport default {\n  name: \"search\",\n  data() {\n    return {\n      searchTasks: [],\n      tasks: [],\n      value: \"\",\n    };\n  },\n  mounted() {\n    this.findTasks();\n  },\n  methods: {\n    changeTask(val) {\n      this.$emit(\"refresh\", val);\n    },\n    dataFilter(val) {\n      if (val) {\n        //val存在\n        this.searchTasks = this.tasks.filter((item) => {\n          var label = this.renderLabel(item);\n          var value = this.renderLabel(item);\n          if (\n            !!~value.indexOf(val) ||\n            !!~value.toUpperCase().indexOf(val.toUpperCase()) ||\n            !!~label.indexOf(val) ||\n            !!~label.toUpperCase().indexOf(val.toUpperCase())\n          ) {\n            return true;\n          }\n        });\n      } else {\n        //val为空时，还原数组\n        this.searchTasks = this.tasks;\n      }\n    },\n    findTasks() {\n      var $this = this;\n      var $url =\n        $this.$config.kts +\n        \"runtime/tasks?includeProcessVariables=true&size=100&sort=createTime&order=desc&assignee=\" +\n        $this.$user.name;\n      sysApi\n        .getAsyncDatas({\n          url: $url,\n          data: {},\n        })\n        .then((res) => {\n          this.searchTasks = res.data;\n          this.tasks = res.data;\n        });\n    },\n    renderLabel(row) {\n      var variables = row.variables || [];\n      var taskName = variables.find((d) => {\n        return d.name == \"taskName\";\n      });\n      return taskName ? taskName.value : \" - \";\n    },\n    renderValue(row) {\n      var variables = row.variables || [];\n      var taskId = variables.find((d) => {\n        return d.name == \"taskId\";\n      });\n      return taskId ? taskId.value : \" - \";\n    },\n    cliam() {\n      var $this = this;\n      var $url = $this.$config.kts + \"task/claim?user=\" + $this.$user.name;\n      sysApi\n        .getAsyncDatas({\n          url: $url,\n          data: {},\n        })\n        .then((res) => {\n          if (res.code == \"0\" && res.result.length) {\n            $this.$notify({\n              title: \"成功\",\n              message: res.message,\n              type: \"success\",\n            });\n            $this.findTasks();\n          } else {\n            $this.$notify({\n              title: \"警告\",\n              message: res.message,\n              type: \"warning\",\n            });\n          }\n        });\n    },\n    save() {\n      console.log(\"保存\");\n    },\n    logout() {\n      userApi\n        .oauth2Client(null, this.$config.kd_auth_server + \"oauth\")\n        .oAuthLoginPage();\n    },\n  },\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped lang=\"scss\">\n.search {\n  width: calc(100% - 20px);\n  height: 40px;\n  float: left;\n  padding: 5px 10px;\n  background-color: #ffffff;\n  padding-bottom: 15px;\n  position: relative;\n  z-index: 4444;\n}\n.el-select__popper {\n  z-index: 4445 !important;\n}\n.search .el-select {\n  float: left;\n}\n.search button {\n  float: left;\n  padding: 0px 15px;\n}\n.search button.btn-cliam {\n  margin-left: 30px;\n}\n.search button.btn-logout {\n  float: right;\n}\n</style>\n"]}]}