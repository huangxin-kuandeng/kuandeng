lect v-model=\"sizeForm.region\" placeholder=\"请选择活动区域\">\n            <el-option label=\"区域一\" value=\"shanghai\"></el-option>\n            <el-option label=\"区域二\" value=\"beijing\"></el-option>\n          </el-select>\n        </el-form-item>\n        <el-form-item label=\"内容描述\">\n          <el-input v-model=\"sizeForm.name\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"标记状态\">\n          <el-radio-group v-model=\"sizeForm.resource\" size=\"medium\">\n            <el-radio border label=\"待核实\"></el-radio>\n            <el-radio border label=\"数据错误，需修改\"></el-radio>\n            <el-radio border label=\"数据正确，误报\"></el-radio>\n            <el-radio border label=\"数据错误，搁置\"></el-radio>\n            <el-radio border label=\"数据错误，已修改\"></el-radio>\n          </el-radio-group>\n        </el-form-item>\n        <el-form-item size=\"large\">\n          <el-button type=\"primary\" size=\"mini\" @click=\"visible = false\"\n            >确定</el-button\n          >\n          <el-button size=\"mini\" type=\"text\" @click=\"visible = false\"\n            >取消</el-button\n          >\n        </el-form-item>\n      </el-form>\n      <template #reference>\n        <div class=\"footHeaderToggle\">\n          <el-tooltip class=\"item\" effect=\"dark\" content=\"修改\" placement=\"top\">\n            <el-button\n              @click.stop=\"visible = true\"\n              icon=\"el-icon-edit-outline\"\n              title=\"修改\"\n            ></el-button>\n          </el-tooltip>\n          <el-tooltip\n            class=\"item\"\n            effect=\"dark\"\n            content=\"质检标记\"\n            placement=\"top\"\n          >\n            <el-button\n              @click.stop=\"visible = true\"\n              icon=\"el-icon-location\"\n              title=\"质检标记\"\n            ></el-button>\n          </el-tooltip>\n          <el-tooltip\n            class=\"item\"\n            effect=\"dark\"\n            content=\"问题标记\"\n            placement=\"top\"\n          >\n            <el-button\n              @click.stop=\"visible = true\"\n              icon=\"el-icon-add-location\"\n              title=\"问题标记\"\n            ></el-button>\n          </el-tooltip>\n        </div>\n      </template>\n    </el-popover>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"footHeader\",\n  data() {\n    return {\n      toggleClass: this.hideHeader\n        ? \"el-icon-caret-top\"\n        : \"el-icon-caret-bottom\",\n      visible: false,\n      sizeForm: {\n        name: \"\",\n        region: \"\",\n        date1: \"\",\n        date2: \"\",\n        delivery: false,\n        type: [],\n        resource: \"\",\n        desc: \"\",\n      },\n    };\n  },\n  props: [\"hideHeader\"],\n  methods: {\n    tableToggle() {\n      this.$emit(\"tableToggle\", this.hideHeader);\n    },\n  },\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped lang=\"scss\">\n.contentEdit {\n  float: left;\n  margin: 0 auto;\n  padding: 0px;\n  position: absolute;\n  right: 5px;\n  bottom: 35px;\n  width: auto;\n}\n.contentEdit .footHeaderToggle button {\n  background-color: rgba(0, 0, 0, 0.4);\n  border: 1px solid rgba(58, 142, 230, 0);\n  min-height: 20px;\n  padding: 5px;\n  font-size: 18px;\n  color: rgba(255, 255, 255, 0.6);\n}\n.contentEdit .footHeaderToggle button:focus,\n.contentEdit .footHeaderToggle button:hover {\n  border: 1px solid rgba(58, 142, 230, 1);\n  color: rgba(255, 255, 255, 1);\n}\n</style>\n"]}]}