()\"\n    ></el-button>\n    <el-button class=\"prePage\" @click=\"prePage()\">上一页</el-button>\n    <div class=\"pageInfo\">\n      <span class=\"PageNum\">{{ pageNum }}</span>\n      <b> / </b>\n      <span class=\"pageTotleNum\">{{ pageTotleNum }}</span>\n    </div>\n    <el-button class=\"nextPage\" @click=\"nextPage()\">下一页</el-button>\n    <el-button\n      class=\"lastPage\"\n      icon=\"el-icon-d-arrow-right\"\n      title=\"尾页\"\n      size=\"medium\"\n      plain\n      @click=\"lastPage()\"\n    ></el-button>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"pages\",\n  data() {\n    return {\n      pageNum: 1,\n    };\n  },\n  props: [\"pageTotleNum\"],\n  methods: {\n    dataFilter() {},\n    firstPage() {\n      this.pageNum = 1;\n      this.$emit(\"pageChange\", this.pageNum);\n    },\n    lastPage() {\n      this.pageNum = this.pageTotleNum;\n      this.$emit(\"pageChange\", this.pageNum);\n    },\n    prePage() {\n      if (this.pageNum > 1) {\n        this.pageNum--;\n        this.$emit(\"pageChange\", this.pageNum);\n      }\n    },\n    nextPage() {\n      if (this.pageNum < this.pageTotleNum) {\n        this.pageNum++;\n        this.$emit(\"pageChange\", this.pageNum);\n      }\n    },\n  },\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped lang=\"scss\">\n.pages {\n  float: left;\n  position: absolute;\n  bottom: 0px;\n  left: 0px;\n  right: 0px;\n  width: 380px;\n  margin: 0 auto;\n  padding: 5px 0px 15px 0px;\n}\n.pages button,\n.pages .pageInfo {\n  float: left;\n  min-height: 30px;\n  height: 30px;\n  line-height: 30px;\n  width: auto;\n  background-color: #f2f4f9;\n}\n.pages .pageInfo {\n  margin: 0px 10px;\n  width: 120px;\n}\n.pages button.prePage,\n.pages button.nextPage {\n  padding: 0px 20px;\n  border-radius: 0px;\n}\n.pages button.firstPage,\n.pages button.lastPage {\n  padding: 0px 5px;\n  border: none;\n  min-height: 30px;\n}\n</style>\n"]}]}