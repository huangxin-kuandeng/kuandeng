ACxC,YAAA,CAGQ,MAHR,EAGQ;AAFN,UAAA,KAAqD,EAArD;AAAA,qBAAA,OAAA;AAAA,qBAAA,SAAA;AAAA,yBAAA;AAAA,WAEM;AADN,UAAA,SAA0B,EAAlB,QAAA,CAAA,WAAA,CAAY,IAAZ;AACF,SAHR,E,IAAA,E;;AAAA,U,aAAA,CADwC,C;;;;OAP1C,E;;AAAA,Q,2BAAA,C;KAYY,CAZZ,E;;AAAA,KAC6B,E;;;;GAV/B,E;;AAAA,I,2CAAA,CAqCI,EAdJ,YAAA,CAMC,oBAND,EAMC;AALC,IAAA,KAAK,EAAC,WAKP;AAJC,IAAA,IAAI,EAAC,SAIN;AAHC,IAAA,IAAI,EAAC,gBAGN;AAFE,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,MAAA,IAAE,QAAA,CAAA,KAAA,EAAF;AAEP,GAND,E;qBAKG,MAAE,C,UAAA,C;;;;GALL,CAcI,EAPJ,YAAA,CAMC,oBAND,EAMC;AALC,IAAA,KAAK,EAAC,YAKP;AAJC,IAAA,IAAI,EAAC,SAIN;AAHC,IAAA,IAAI,EAAC,gBAGN;AAFE,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,MAAA,IAAE,QAAA,CAAA,MAAA,EAAF;AAEP,GAND,E;qBAKG,MAAE,C,UAAA,C;;;;GALL,CAOI,CAtCN,C","sourcesContent":["<template>\n  <div class=\"search\">\n    <el-select\n      v-model=\"value\"\n      placeholder=\"请选择\"\n      filterable\n      :filter-method=\"dataFilter\"\n      @change=\"changeTask\"\n      no-data-text=\"无任务\"\n      no-match-text=\"无匹配任务\"\n    >\n      <el-option\n        v-for=\"item in searchTasks\"\n        :key=\"renderValue(item)\"\n        :label=\"renderLabel(item)\"\n        :value=\"renderValue(item)\"\n        :title=\"renderLabel(item)\"\n      >\n        <span v-html=\"renderLabel(item)\"></span>\n        <span\n          style=\"float: right; color: #8492a6; font-size: 13px\"\n          v-html=\"renderValue(item)\"\n        ></span>\n      </el-option>\n    </el-select>\n    <el-button\n      class=\"btn-cliam\"\n      type=\"primary\"\n      icon=\"el-icon-search\"\n      @click=\"cliam()\"\n      >领取</el-button\n    >\n    <el-button\n      class=\"btn-logout\"\n      type=\"primary\"\n      icon=\"el-icon-search\"\n      @click=\"logout()\"\n      >退出</el-button\n    >\n  </div>\n</template>\n\n<script>\nimport * as sysApi from \"../services/sys\";\nimport * as userApi from \"../services/jquery.oauth2-client\";\nexport default {\n  name: \"search\",\n  data() {\n    return {\n      catchTasks: {},\n      searchTasks: [],\n      tasks: [],\n      value: \"\",\n    };\n  },\n  mounted() {\n    this.findTasks();\n  },\n  methods: {\n    changeTask(val) {\n      this.$emit(\"refresh\", val);\n    },\n    dataFilter(val) {\n      if (val) {\n        //val存在\n        this.searchTasks = this.tasks.filter((item) => {\n          var label = this.renderLabel(item);\n          var value = this.renderLabel(item);\n          if (\n            !!~value.indexOf(val) ||\n            !!~value.toUpperCase().indexOf(val.toUpperCase()) ||\n            !!~label.indexOf(val) ||\n            !!~label.toUpperCase().indexOf(val.toUpperCase())\n          ) {\n            return true;\n          }\n        });\n      } else {\n        //val为空时，还原数组\n        this.searchTasks = this.tasks;\n      }\n    },\n    // 第一次查询任务信息--部分信息（通过用户）\n    findTasks() {\n      var $this = this;\n      var $url =\n        $this.$config.kts +\n        \"runtime/tasks?includeProcessVariables=true&size=100&sort=createTime&order=desc&assignee=\" +\n        $this.$user.name;\n      $this.catchTasks = {};\n      $this.searchTasks = [];\n      $this.tasks = [];\n      sysApi\n        .getAsyncDatas({\n          url: $url,\n          data: {},\n        })\n        .then((res) => {\n          var datas = res.data || [];\n          datas.forEach((d) => {\n            var taskId = d.variables.find((s) => {\n              return s.name == \"taskId\";\n            });\n            if (taskId) {\n              $this.catchTasks[taskId.value] = d;\n            }\n          });\n          $this.findTaskLists();\n        });\n    },\n    // 第二次查询任务信息--详细信息（通过id）\n    findTaskLists() {\n      var $this = this;\n\n      var formData = new FormData();\n      var ids = [];\n\n      for (var taskId in $this.catchTasks) {\n        ids.push(taskId);\n      }\n      if (!ids.length) {\n        return;\n      }\n      formData.append(\"ids\", ids.join(\",\"));\n      var $url = $this.$config.kts + \"task/findByIds\";\n      sysApi\n        .postAsyncDatas({\n          url: $url,\n          data: formData,\n        })\n        .then((res) => {\n          var results = res.result || {};\n          for (var id in results) {\n            var featureGroups = results[id].featureGroups || [];\n            var taskDefinitionKey = $this.catchTasks[id].taskDefinitionKey;\n            var feature = featureGroups.find((d) => {\n              return d.linkId == taskDefinitionKey;\n            });\n            if (feature) {\n              results[id][\"featureGroupId\"] = feature.featureGroupId;\n              results[id][\"taskId\"] = id;\n              results[id][\"taskName\"] = results[id].name;\n              $this.searchTasks.push(results[id]);\n              $this.tasks.push(results[id]);\n            }\n          }\n        });\n      console.log($this.catchTasks);\n    },\n    renderLabel(row) {\n      return row.taskName || \" - \";\n    },\n    renderValue(row) {\n      return row.taskId || \" - \";\n    },\n    cliam() {\n      var $this = this;\n      var $url = $this.$config.kts + \"task/claim?user=\" + $this.$user.name;\n      sysApi\n        .getAsyncDatas({\n          url: $url,\n          data: {},\n        })\n        .then((res) => {\n          if (res.code == \"0\" && res.result.length) {\n            $this.$notify({\n              title: \"成功\",\n              message: res.message,\n              type: \"success\",\n            });\n            $this.findTasks();\n          } else {\n            $this.$notify({\n              title: \"警告\",\n              message: res.message,\n              type: \"warning\",\n            });\n          }\n        });\n    },\n    save() {\n      console.log(\"保存\");\n    },\n    logout() {\n      userApi\n        .oauth2Client(null, this.$config.kd_auth_server + \"oauth\")\n        .oAuthLoginPage();\n    },\n  },\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped lang=\"scss\">\n.search {\n  width: calc(100% - 20px);\n  height: 40px;\n  float: left;\n  padding: 5px 10px;\n  background-color: #ffffff;\n  padding-bottom: 15px;\n  position: relative;\n  z-index: 4444;\n}\n.el-select__popper {\n  z-index: 4445 !important;\n}\n.search .el-select {\n  float: left;\n}\n.search button {\n  float: left;\n  padding: 0px 15px;\n}\n.search button.btn-cliam {\n  margin-left: 30px;\n}\n.search button.btn-logout {\n  float: right;\n}\n</style>\n"],"sourceRoot":""},"metadata":{},"sourceType":"module"}