{"ast":null,"code":"export default {\n  name: \"pages\",\n  props: {\n    totle: Number\n  },\n\n  data() {\n    return {\n      pageNum: 1,\n      pageTotleNum: 20\n    };\n  },\n\n  methods: {\n    dataFilter() {},\n\n    firstPage() {\n      this.pageNum = 1;\n      this.$emit(\"pageChange\", this.pageNum);\n    },\n\n    lastPage() {\n      this.pageNum = this.pageTotleNum;\n      this.$emit(\"pageChange\", this.pageNum);\n    },\n\n    prePage() {\n      if (this.pageNum > 1) {\n        this.pageNum--;\n        this.$emit(\"pageChange\", this.pageNum);\n      }\n    },\n\n    nextPage() {\n      if (this.pageNum < this.pageTotleNum) {\n        this.pageNum++;\n        this.$emit(\"pageChange\", this.pageNum);\n      }\n    }\n\n  }\n};","map":{"version":3,"sources":["D:\\project\\batch-edit\\src\\components\\contentComponents\\contentPage.vue"],"names":[],"mappings":"AA6BA,eAAe;AACb,EAAA,IAAI,EAAE,OADO;AAEb,EAAA,KAAK,EAAE;AACL,IAAA,KAAK,EAAE;AADF,GAFM;;AAKb,EAAA,IAAI,GAAG;AACL,WAAO;AACL,MAAA,OAAO,EAAE,CADJ;AAEL,MAAA,YAAY,EAAE;AAFT,KAAP;AAID,GAVY;;AAWb,EAAA,OAAO,EAAE;AACP,IAAA,UAAU,GAAG,CAAE,CADR;;AAEP,IAAA,SAAS,GAAG;AACV,WAAK,OAAL,GAAe,CAAf;AACA,WAAK,KAAL,CAAW,YAAX,EAAyB,KAAK,OAA9B;AACD,KALM;;AAMP,IAAA,QAAQ,GAAG;AACT,WAAK,OAAL,GAAe,KAAK,YAApB;AACA,WAAK,KAAL,CAAW,YAAX,EAAyB,KAAK,OAA9B;AACD,KATM;;AAUP,IAAA,OAAO,GAAG;AACR,UAAI,KAAK,OAAL,GAAe,CAAnB,EAAsB;AACpB,aAAK,OAAL;AACA,aAAK,KAAL,CAAW,YAAX,EAAyB,KAAK,OAA9B;AACF;AACD,KAfM;;AAgBP,IAAA,QAAQ,GAAG;AACT,UAAI,KAAK,OAAL,GAAe,KAAK,YAAxB,EAAsC;AACpC,aAAK,OAAL;AACA,aAAK,KAAL,CAAW,YAAX,EAAyB,KAAK,OAA9B;AACF;AACD;;AArBM;AAXI,CAAf","sourcesContent":["<template>\n  <div class=\"pages\">\n    <el-button\n      class=\"firstPage\"\n      icon=\"el-icon-d-arrow-left\"\n      title=\"首页\"\n      size=\"medium\"\n      plain\n      @click=\"firstPage()\"\n    ></el-button>\n    <el-button class=\"prePage\" @click=\"prePage()\">上一页</el-button>\n    <div class=\"pageInfo\">\n      <span class=\"PageNum\">{{ pageNum }}</span>\n      <b> / </b>\n      <span class=\"pageTotleNum\">{{ totle }}</span>\n    </div>\n    <el-button class=\"nextPage\" @click=\"nextPage()\">下一页</el-button>\n    <el-button\n      class=\"lastPage\"\n      icon=\"el-icon-d-arrow-right\"\n      title=\"尾页\"\n      size=\"medium\"\n      plain\n      @click=\"lastPage()\"\n    ></el-button>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"pages\",\n  props: {\n    totle: Number,\n  },\n  data() {\n    return {\n      pageNum: 1,\n      pageTotleNum: 20,\n    };\n  },\n  methods: {\n    dataFilter() {},\n    firstPage() {\n      this.pageNum = 1;\n      this.$emit(\"pageChange\", this.pageNum);\n    },\n    lastPage() {\n      this.pageNum = this.pageTotleNum;\n      this.$emit(\"pageChange\", this.pageNum);\n    },\n    prePage() {\n      if (this.pageNum > 1) {\n        this.pageNum--;\n        this.$emit(\"pageChange\", this.pageNum);\n      }\n    },\n    nextPage() {\n      if (this.pageNum < this.pageTotleNum) {\n        this.pageNum++;\n        this.$emit(\"pageChange\", this.pageNum);\n      }\n    },\n  },\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped lang=\"scss\">\n.pages {\n  float: left;\n  position: absolute;\n  bottom: 0px;\n  left: 0px;\n  right: 0px;\n  width: 380px;\n  margin: 0 auto;\n  padding: 5px 0px 10px 0px;\n}\n.pages button,\n.pages .pageInfo {\n  float: left;\n  min-height: 30px;\n  height: 30px;\n  line-height: 30px;\n  width: auto;\n}\n.pages .pageInfo {\n  margin: 0px 10px;\n  width: 120px;\n}\n.pages button.prePage,\n.pages button.nextPage {\n  padding: 0px 20px;\n  border-radius: 0px;\n}\n.pages button.firstPage,\n.pages button.lastPage {\n  padding: 0px 5px;\n  border: none;\n  min-height: 30px;\n}\n</style>\n"],"sourceRoot":""},"metadata":{},"sourceType":"module"}