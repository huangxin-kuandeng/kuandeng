eturn d.k == key;\n        });\n\n        if (editTag) {\n          editTag.v = form[key];\n        } else {\n          this.editHistory[id].tag.push({\n            k: key,\n            v: form[key]\n          });\n        }\n      }\n\n      console.log(this.editHistory[id]);\n    },\n\n    finishTask() {\n      var list = [];\n\n      for (var id in this.editHistory) {\n        list.push(this.editHistory[id]);\n      }\n\n      if (!list.length) {\n        return;\n      }\n\n      var $this = this;\n      $this.fullscreenLoading = true;\n      var $url = $this.$config.kds_data + \"data/tags/edit?keys=lane_400951053_1&namespace=hdmap\";\n      sysApi.postAsyncDatas({\n        url: $url,\n        data: list\n      }).then(res => {\n        $this.message(res.code, res.message);\n        $this.fullscreenLoading = false;\n        console.log(\"完成\"); // if(res.code != \"0\"){\n        // }\n      });\n    },\n\n    refresh() {\n      this.pageChange(this.pageNum);\n    },\n\n    pageChange(page) {\n      if (page) {\n        this.pageNum = page;\n      }\n\n      var $this = this;\n      var $url = $this.$config.kds_data + \"data/queryPage?keys=lane_400951053_1&namespace=hdmap\";\n      $this.fullscreenLoading = true;\n      sysApi.postAsyncDatas({\n        url: $url,\n        data: {\n          ops: [],\n          page: {\n            ops: [{\n              k: \"modelName\",\n              type: \"string\",\n              v: \"DIVIDER\",\n              op: \"in\"\n            }],\n            sort: [{\n              k: \"DIVIDER.SOURCE\",\n              order: \"desc\"\n            }, {\n              k: \"DIVIDER.R_LINE\",\n              order: \"desc\"\n            }],\n            page: {\n              pageNo: this.pageNum,\n              pageSize: 20\n            }\n          }\n        }\n      }).then(res => {\n        $this.message(res.code, res.message);\n        $this.fullscreenLoading = false;\n        $this.pageTotleNum = res.result.page.totalPages || 0;\n        $this.activeLists[$this.activeName] = res.result.data || [];\n      });\n    },\n\n    findLists(value) {\n      // pageNum\n      console.log(value);\n      var $this = this;\n      var $url = $this.$config.kds_data + \"data/queryPage?keys=lane_400951053_1&namespace=hdmap\";\n      $this.fullscreenLoading = true;\n      sysApi.postAsyncDatas({\n        url: $url,\n        data: {\n          ops: [],\n          page: {\n            pageSize: 10,\n            pageNo: 1\n          }\n        }\n      }).then(function (res) {\n        $this.message(res.code, res.message);\n        $this.fullscreenLoading = false;\n        $this.pageChange(value); // $this.activeName = $this.lists[0].value;\n        // $this.lists = [\n        //   {\n        //     name: \"路牌\",\n        //     value: \"1\",\n        //   },\n        //   {\n        //     name: \"灯杆\",\n        //     value: \"2\",\n        //   },\n        //   {\n        //     name: \"车道线\",\n        //     value: \"3\",\n        //   },\n        //   {\n        //     name: \"地面定位目标\",\n        //     value: \"4\",\n        //   },\n        // ];\n      });\n    },\n\n    message(type, message) {\n      if (type == \"0\") {\n        ElMessage.success({\n          message: message,\n          type: \"success\"\n        });\n      } else {\n        ElMessage.error(\"错误信息：\" + message);\n      }\n    }\n\n  },\n  components: {\n    contentSearch,\n    contentPage,\n    contentEdit\n  }\n};","map":{"version":3,"sources":["D:\\project\\batch-edit\\src\\components\\content.vue"],"names":[],"mappings":"AA0CA,OAAO,aAAP,MAA0B,kDAA1B;AACA,OAAO,WAAP,MAAwB,gDAAxB;AACA,OAAO,WAAP,MAAwB,gDAAxB;AACA,OAAO,KAAK,MAAZ,MAAwB,iBAAxB;AACA,SAAS,SAAT,QAA0B,cAA1B;AACA,eAAe;AACb,EAAA,IAAI,EAAE,QADO;;AAEb,EAAA,IAAI,GAAG;AACL,WAAO;AACL,MAAA,iBAAiB,EAAE,KADd;AAEL,MAAA,KAAK,EAAE,CACL;AACE,QAAA,IAAI,EAAE,IADR;AAEE,QAAA,KAAK,EAAE,UAFT;AAGE,QAAA,SAAS,EAAE;AAHb,OADK,EAML;AACE,QAAA,IAAI,EAAE,IADR;AAEE,QAAA,KAAK,EAAE,UAFT;AAGE,QAAA,SAAS,EAAE