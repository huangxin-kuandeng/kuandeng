{"ast":null,"code":"import contentSearch from \"@/components/contentComponents/contentSearch.vue\";\nimport contentPage from \"@/components/contentComponents/contentPage.vue\";\nimport contentEdit from \"@/components/contentComponents/contentEdit.vue\";\nimport * as sysApi from \"../services/sys\";\nimport * as util from \"../services/util\";\nimport { ElMessage } from \"element-plus\";\nexport default {\n  name: \"search\",\n\n  data() {\n    return {\n      editDisabled: false,\n      fullscreenLoading: false,\n      lists: [{\n        name: \"数据\",\n        value: \"DATA\",\n        modelName: \"DATA\"\n      }],\n      activeLists: [],\n      value: \"\",\n      taskId: null,\n      pageTotleNum: 0,\n      editHistory: {},\n      pageNum: 0,\n      activeName: \"DATA\"\n    };\n  },\n\n  mounted() {\n    this.refresh(1); // editDisabled：属性编辑栏根据用户权限验证是否可修改\n\n    var role = util.isRoleAuth(this.$user);\n\n    if ([1, 3].includes(role)) {\n      this.editDisabled = false;\n    } else {\n      this.editDisabled = true;\n    }\n  },\n\n  methods: {\n    // 模型类型切换\n    handleClick(tab) {\n      console.log(tab);\n      return;\n    },\n\n    mouseenter(event) {\n      event.currentTarget.className = \"blockImage show\";\n    },\n\n    mouseleave(event) {\n      event.currentTarget.className = \"blockImage hide\";\n    },\n\n    logout() {\n      console.log(\"退出\");\n    },\n\n    // 是否被标记的状态\n    renderMarkType(tag) {\n      if (!tag) {\n        return;\n      }\n\n      var key = \"FLAGTO\";\n      var editTag = tag.find(d => {\n        return d.k == key;\n      });\n\n      if (editTag && editTag.v == \"1\") {\n        return true;\n      }\n\n      return false;\n    },\n\n    editList(id, form) {\n      this.eidtTask(id, form);\n    },\n\n    checkList(id, form) {\n      this.eidtTask(id, form);\n    },\n\n    tagImage(id) {\n      var key = \"FLAGTO\";\n\n      if (!this.editHistory[id]) {\n        var activeList = this.activeLists[this.activeName];\n        var activeData = activeList.find(d => {\n          return d.id == id;\n        });\n        this.editHistory[id] = activeData;\n      }\n\n      var editTag = this.editHistory[id].tag.find(d => {\n        return d.k == key;\n      });\n\n      if (editTag) {\n        editTag.v = editTag.v == \"2\" ? \"1\" : \"2\";\n      } else {\n        this.editHistory[id].tag.push({\n          k: key,\n          v: \"1\"\n        });\n      }\n    },\n\n    // 修改属性\n    eidtTask(id, form) {\n      if (!this.editHistory[id]) {\n        var activeList = this.activeLists[this.activeName];\n        var activeData = activeList.find(d => {\n          return d.id == id;\n        });\n        this.editHistory[id] = activeData;\n      }\n\n      for (var key in form) {\n        var editTag = this.editHistory[id].tag.find(d => {\n          return d.k == key;\n        });\n\n        if (editTag) {\n          editTag.v = form[key];\n        } else {\n          this.editHistory[id].tag.push({\n            k: key,\n            v: form[key]\n          });\n        }\n      }\n\n      console.log(this.editHistory[id]);\n    },\n\n    // 完成保存所修改的数据\n    finishTask() {\n      var list = [];\n\n      for (var id in this.editHistory) {\n        list.push(this.editHistory[id]);\n      }\n\n      if (!list.length) {\n        return;\n      }\n\n      var $this = this;\n      $this.fullscreenLoading = true;\n      var $url = $this.$config.kds_data + \"data/tags/edit?keys=lane_400951053_1&namespace=hdmap\";\n      sysApi.postAsyncDatas({\n        url: $url,\n        data: list\n      }).then(res => {\n        $this.message(res.code, res.message);\n        $this.fullscreenLoading = false;\n        console.log(\"完成\");\n\n        if (res.code == \"0\") {\n          $this.taskId = null;\n          $this.$emit(\"refresh\");\n        }\n      });\n    },\n\n    // 刷新数据\n    refresh(param = {}) {\n      var page = param.page || this.pageNum;\n\n      if (param.id) {\n        this.taskId = param.id;\n      }\n\n      this.findLists(page);\n    },\n\n    // 刷新报表\n    refreshForm() {\n      this.$emit(\"refreshForm\", this.taskId);\n    },\n\n    // 分页查询\n    pageChange(page) {\n      this.pageTotleNum = 0;\n      this.activeLists = [];\n\n      if (!this.taskId) {\n        return;\n      }\n\n      if (page) {\n        this.pageNum = page;\n      }\n\n      var $this = this;\n      var $url = $this.$config.kds_data + \"data/queryPage?keys=lane_400951053_1&namespace=hdmap\";\n      $this.fullscreenLoading = true;\n      sysApi.postAsyncDatas({\n        url: $url,\n        data: {\n          ops: [],\n          page: {\n            ops: [{\n              k: \"modelName\",\n              type: \"string\",\n              v: \"DIVIDER\",\n              op: \"in\"\n            }],\n            sort: [{\n              k: \"DIVIDER.SOURCE\",\n              order: \"desc\"\n            }, {\n              k: \"DIVIDER.R_LINE\",\n              order: \"desc\"\n            }],\n            page: {\n              pageNo: this.pageNum,\n              pageSize: 20\n            }\n          }\n        }\n      }).then(res => {\n        res = {\n          code: \"0\",\n          message: \"成功\",\n          error: null,\n          result: [{\n            modelName: \"DIVIVER\",\n            groupKeys: {\n              TYPE: \"1\",\n              SUBTYPE: \"1\"\n            },\n            datas: [{\n              id: \"60da9dc4ff622a279e1fab1d\",\n              entityId: 1,\n              key: \"lane_1_1\",\n              taskId: \"1_1\",\n              autoProjectId: \"1\",\n              featureGroupId: \"1\",\n              entityType: \"way\",\n              modelName: \"DIVIVER\",\n              tags: {\n                TYPE: \"1\",\n                SUBTYPE: \"1\",\n                ROAND_NAME: \"亮马桥路1\"\n              },\n              layerId: null,\n              changesetId: 1,\n              visible: true,\n              timestamp: 1550732121186,\n              version: 1,\n      