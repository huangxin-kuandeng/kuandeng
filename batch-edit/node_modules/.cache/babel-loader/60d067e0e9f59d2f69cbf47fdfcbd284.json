ds: {\n    handleClick(tab, event) {\n      if (!this.activeLists[tab.props.name].length) {\n        this.pageChange(1);\n      }\n    },\n    cliam() {\n      console.log(\"领取\");\n    },\n    save() {\n      console.log(\"保存\");\n    },\n    logout() {\n      console.log(\"退出\");\n    },\n    editList(id, type) {\n      console.log(\"编辑\");\n      console.log(id);\n      console.log(type);\n    },\n    pageChange(page) {\n      // pageNum\n      console.log(page);\n      console.log(this.activeName);\n      let $this = this;\n      let f_url = \"http://192.168.7.22:23370/kcms/collection/project/queryPage\";\n      $this.$axios\n        .post(f_url, {\n          ops: [],\n          page: {\n            pageSize: 10,\n            pageNo: page,\n          },\n        })\n        .then(function (response) {\n          console.log(response.data);\n          $this.pageTotleNum = response.data.result.page.totalPages || 0;\n          $this.activeLists[$this.activeName] = response.data.result.data || [];\n        })\n        .catch(function (error) {\n          console.log(error);\n        });\n    },\n    findLists(value) {\n      // pageNum\n      console.log(value);\n      let $this = this;\n      let f_url = \"http://192.168.7.22:23370/kcms/collection/project/queryPage\";\n      $this.$axios\n        .post(f_url, {\n          ops: [],\n          page: {\n            pageSize: 10,\n            pageNo: 1,\n          },\n        })\n        .then(function (response) {\n          console.log(response);\n          $this.lists = [\n            {\n              name: \"路牌\",\n              value: \"1\",\n            },\n            {\n              name: \"灯杆\",\n              value: \"2\",\n            },\n            {\n              name: \"车道线\",\n              value: \"3\",\n            },\n            {\n              name: \"地面定位目标\",\n              value: \"4\",\n            },\n          ];\n        })\n        .catch(function (error) {\n          console.log(error);\n        });\n    },\n  },\n  components: {\n    contentSearch,\n    contentPage,\n    contentEdit,\n  },\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style lang=\"scss\">\n.tabs {\n  float: left;\n  width: 100%;\n  margin: 10px 0px;\n  height: calc(100% - 140px);\n  border-bottom: 1px solid #e4e7ed;\n}\n.tabs .contentTabs {\n  height: 100%;\n}\n.tabs .contentTabs .el-tabs__content {\n  height: calc(100% - 50px);\n  padding-top: 10px;\n  background-color: #f2f4f9;\n}\n.tabs .contentTabs .el-tabs__content .el-tab-pane {\n  height: 100%;\n}\n.tabs .blockImage {\n  width: calc(20% - 30px);\n  margin: 0px 15px;\n  height: 200px;\n  float: left;\n  position: relative;\n}\n.tabs .blockImage img {\n  border: 2px solid rgba(58, 142, 230, 0);\n}\n.tabs .blockImage img:hover {\n  border: 2px solid rgba(58, 142, 230, 1);\n}\n.tabs .blockImage .demonstration {\n  float: left;\n  height: 30px;\n  position: absolute;\n  bottom: 0px;\n  width: 100%;\n  left: 0px;\n  line-height: 30px;\n  font-size: 16px;\n}\n</style>\n"],"sourceRoot":""},"metadata":{},"sourceType":"module"}