{"ast":null,"code":"import * as sysApi from \"../services/sys\";\nconst copyCities = [{\n  value: \"Beijing\",\n  label: \"北京\"\n}, {\n  value: \"Shanghai\",\n  label: \"上海\"\n}, {\n  value: \"Nanjing\",\n  label: \"南京\"\n}, {\n  value: \"Chengdu\",\n  label: \"成都\"\n}, {\n  value: \"Shenzhen\",\n  label: \"深圳\"\n}, {\n  value: \"Guangzhou\",\n  label: \"广州\"\n}];\nexport default {\n  name: \"search\",\n\n  data() {\n    return {\n      cities: [{\n        value: \"Beijing\",\n        label: \"北京\"\n      }, {\n        value: \"Shanghai\",\n        label: \"上海\"\n      }, {\n        value: \"Nanjing\",\n        label: \"南京\"\n      }, {\n        value: \"Chengdu\",\n        label: \"成都\"\n      }, {\n        value: \"Shenzhen\",\n        label: \"深圳\"\n      }, {\n        value: \"Guangzhou\",\n        label: \"广州\"\n      }],\n      value: \"\"\n    };\n  },\n\n  mounted() {\n    this.findTasks();\n  },\n\n  methods: {\n    dataFilter(val) {\n      if (val) {\n        //val存在\n        this.cities = copyCities.filter(item => {\n          if (!!~item.value.indexOf(val) || !!~item.value.toUpperCase().indexOf(val.toUpperCase()) || !!~item.label.indexOf(val) || !!~item.label.toUpperCase().indexOf(val.toUpperCase())) {\n            return true;\n          }\n        });\n      } else {\n        //val为空时，还原数组\n        this.cities = copyCities;\n      }\n    },\n\n    findTasks() {\n      var $this = this;\n      var $url = $this.$config.kts + \"task/manual/query?size=10&sort=createTime&order=desc&start=0\";\n      sysApi.postAsyncDatas({\n        url: $url,\n        data: {\n          includeProcessVariables: \"true\",\n          taskDefinitionKey: \"work\",\n          descriptionLike: null,\n          unassigned: true,\n          processInstanceVariables: [{\n            name: \"starter\",\n            value: \"guojing1\",\n            operation: \"equals\",\n            type: \"string\"\n          }]\n        }\n      }).then(res => {\n        console.log(res);\n      });\n    },\n\n    cliam() {\n      console.log(\"领取\");\n    },\n\n    save() {\n      console.log(\"保存\");\n    },\n\n    logout() {\n      console.log(\"退出\");\n    }\n\n  }\n};","map":{"version":3,"sources":["D:\\project\\batch-edit\\src\\components\\search.vue"],"names":[],"mappings":"AAsCA,OAAO,KAAK,MAAZ,MAAwB,iBAAxB;AACA,MAAM,UAAS,GAAI,CACjB;AACE,EAAA,KAAK,EAAE,SADT;AAEE,EAAA,KAAK,EAAE;AAFT,CADiB,EAKjB;AACE,EAAA,KAAK,EAAE,UADT;AAEE,EAAA,KAAK,EAAE;AAFT,CALiB,EASjB;AACE,EAAA,KAAK,EAAE,SADT;AAEE,EAAA,KAAK,EAAE;AAFT,CATiB,EAajB;AACE,EAAA,KAAK,EAAE,SADT;AAEE,EAAA,KAAK,EAAE;AAFT,CAbiB,EAiBjB;AACE,EAAA,KAAK,EAAE,UADT;AAEE,EAAA,KAAK,EAAE;AAFT,CAjBiB,EAqBjB;AACE,EAAA,KAAK,EAAE,WADT;AAEE,EAAA,KAAK,EAAE;AAFT,CArBiB,CAAnB;AA0BA,eAAe;AACb,EAAA,IAAI,EAAE,QADO;;AAEb,EAAA,IAAI,GAAG;AACL,WAAO;AACL,MAAA,MAAM,EAAE,CACN;AACE,QAAA,KAAK,EAAE,SADT;AAEE,QAAA,KAAK,EAAE;AAFT,OADM,EAKN;AACE,QAAA,KAAK,EAAE,UADT;AAEE,QAAA,KAAK,EAAE;AAFT,OALM,EASN;AACE,QAAA,KAAK,EAAE,SADT;AAEE,QAAA,KAAK,EAAE;AAFT,OATM,EAaN;AACE,QAAA,KAAK,EAAE,SADT;AAEE,QAAA,KAAK,EAAE;AAFT,OAbM,EAiBN;AACE,QAAA,KAAK,EAAE,UADT;AAEE,QAAA,KAAK,EAAE;AAFT,OAjBM,EAqBN;AACE,QAAA,KAAK,EAAE,WADT;AAEE,QAAA,KAAK,EAAE;AAFT,OArBM,CADH;AA2BL,MAAA,KAAK,EAAE;AA3BF,KAAP;AA6BD,GAhCY;;AAiCb,EAAA,OAAO,GAAG;AACR,SAAK,SAAL;AACD,GAnCY;;AAoCb,EAAA,OAAO,EAAE;AACP,IAAA,UAAU,CAAC,GAAD,EAAM;AACd,UAAI,GAAJ,EAAS;AACP;AACA,aAAK,MAAL,GAAc,UAAU,CAAC,MAAX,CAAmB,IAAD,IAAU;AACxC,cACE,CAAC,CAAC,CAAC,IAAI,CAAC,KAAL,CAAW,OAAX,CAAmB,GAAnB,CAAH,IACA,CAAC,CAAC,CAAC,IAAI,CAAC,KAAL,CAAW,WAAX,GAAyB,OAAzB,CAAiC,GAAG,CAAC,WAAJ,EAAjC,CADH,IAEA,CAAC,CAAC,CAAC,IAAI,CAAC,KAAL,CAAW,OAAX,CAAmB,GAAnB,CAFH,IAGA,CAAC,CAAC,CAAC,IAAI,CAAC,KAAL,CAAW,WAAX,GAAyB,OAAzB,CAAiC,GAAG,CAAC,WAAJ,EAAjC,CAJL,EAKE;AACA,mBAAO,IAAP;AACF;AACD,SATa,CAAd;AAUF,OAZA,MAYO;AACL;AACA,aAAK,MAAL,GAAc,UAAd;AACF;AACD,KAlBM;;AAmBP,IAAA,SAAS,GAAG;AACV,UAAI,KAAI,GAAI,IAAZ;AACA,UAAI,IAAG,GACL,KAAK,CAAC,OAAN,CAAc,GAAd,GACA,8DAFF;AAGA,MAAA,MAAK,CACF,cADH,CACkB;AACd,QAAA,GAAG,EAAE,IADS;AAEd,QAAA,IAAI,EAAE;AACJ,UAAA