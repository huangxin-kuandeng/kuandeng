el-option label=\"区域二\" value=\"beijing\"></el-option>\n          </el-select>\n        </el-form-item>\n        <el-form-item label=\"内容描述\">\n          <el-input v-model=\"sizeForm.name\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"标记状态\">\n          <el-radio-group v-model=\"sizeForm.resource\" size=\"medium\">\n            <el-radio border label=\"待核实\"></el-radio>\n            <el-radio border label=\"数据错误，需修改\"></el-radio>\n            <el-radio border label=\"数据正确，误报\"></el-radio>\n            <el-radio border label=\"数据错误，搁置\"></el-radio>\n            <el-radio border label=\"数据错误，已修改\"></el-radio>\n          </el-radio-group>\n        </el-form-item>\n        <el-form-item size=\"large\">\n          <el-button type=\"primary\" size=\"mini\" @click=\"visible = false\"\n            >确定</el-button\n          >\n          <el-button size=\"mini\" type=\"text\" @click=\"visible = false\"\n            >取消</el-button\n          >\n        </el-form-item>\n      </el-form>\n      <template #reference>\n        <div class=\"footHeaderToggle\">\n          <el-tooltip class=\"item\" effect=\"dark\" content=\"修改\" placement=\"top\">\n            <el-button\n              @click.stop=\"visible = true\"\n              icon=\"el-icon-edit-outline\"\n              title=\"修改\"\n            ></el-button>\n          </el-tooltip>\n          <el-tooltip\n            class=\"item\"\n            effect=\"dark\"\n            content=\"质检标记\"\n            placement=\"top\"\n          >\n            <el-button\n              @click.stop=\"visible = true\"\n              icon=\"el-icon-location\"\n              title=\"质检标记\"\n            ></el-button>\n          </el-tooltip>\n          <el-tooltip\n            class=\"item\"\n            effect=\"dark\"\n            content=\"问题标记\"\n            placement=\"top\"\n          >\n            <el-button\n              @click.stop=\"visible = true\"\n              icon=\"el-icon-add-location\"\n              ti