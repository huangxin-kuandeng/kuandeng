{"ast":null,"code":"import contentSearch from \"@/components/contentComponents/contentSearch.vue\";\nimport contentPage from \"@/components/contentComponents/contentPage.vue\";\nimport contentEdit from \"@/components/contentComponents/contentEdit.vue\";\nimport * as sysApi from \"../services/sys\";\nimport { ElMessage } from \"element-plus\";\nexport default {\n  name: \"search\",\n\n  data() {\n    return {\n      fullscreenLoading: false,\n      lists: [{\n        name: \"路牌\",\n        value: \"DIVIDER3\",\n        modelName: \"DIVIDER3\"\n      }, {\n        name: \"灯杆\",\n        value: \"DIVIDER2\",\n        modelName: \"DIVIDER2\"\n      }, {\n        name: \"车道线\",\n        value: \"DIVIDER\",\n        modelName: \"DIVIDER\"\n      }, {\n        name: \"地面定位目标\",\n        value: \"DIVIDER1\",\n        modelName: \"DIVIDER1\"\n      }],\n      activeLists: {},\n      value: \"\",\n      taskId: null,\n      pageTotleNum: 0,\n      editHistory: {},\n      pageNum: 0,\n      activeName: \"DIVIDER\"\n    };\n  },\n\n  mounted() {\n    this.refresh(1);\n  },\n\n  methods: {\n    // 模型类型切换\n    handleClick(tab) {\n      if (!this.activeLists[tab.props.name].length) {\n        this.pageChange(1);\n      }\n    },\n\n    mouseenter(event) {\n      event.currentTarget.className = \"blockImage show\";\n    },\n\n    mouseleave(event) {\n      event.currentTarget.className = \"blockImage hide\";\n    },\n\n    logout() {\n      console.log(\"退出\");\n    },\n\n    // 是否被标记的状态\n    renderMarkType(tag) {\n      var key = \"FLAG\";\n      var editTag = tag.find(d => {\n        return d.k == key;\n      });\n\n      if (editTag && editTag.v == \"1\") {\n        return true;\n      }\n\n      return false;\n    },\n\n    editList(id, form) {\n      this.eidtTask(id, form);\n    },\n\n    checkList(id, form) {\n      this.eidtTask(id, form);\n    },\n\n    tagImage(id) {\n      var key = \"FLAG\";\n\n      if (!this.editHistory[id]) {\n        var activeList = this.activeLists[this.activeName];\n        var activeData = activeList.find(d => {\n          return d.id == id;\n        });\n        this.editHistory[id] = activeData;\n      }\n\n      var editTag = this.editHistory[id].tag.find(d => {\n        return d.k == key;\n      });\n\n      if (editTag) {\n        editTag.v = editTag.v == \"2\" ? \"1\" : \"2\";\n      } else {\n        this.editHistory[id].tag.push({\n          k: key,\n          v: \"1\"\n        });\n      }\n    },\n\n    // 修改属性\n    eidtTask(id, form) {\n      if (!this.editHistory[id]) {\n        var activeList = this.activeLists[this.activeName];\n        var activeData = activeList.find(d => {\n          return d.id == id;\n        });\n        this.editHistory[id] = activeData;\n      }\n\n      for (var key in form) {\n        var editTag = this.editHistory[id].tag.find(d => {\n          return d.k == key;\n        });\n\n        if (editTag) {\n          editTag.v = form[key];\n        } else {\n          this.editHistory[id].tag.push({\n            k: key,\n            v: form[key]\n          });\n        }\n      }\n\n      console.log(this.editHistory[id]);\n    },\n\n    // 完成保存所修改的数据\n    finishTask() {\n      var list = [];\n\n      for (var id in this.editHistory) {\n        list.push(this.editHistory[id]);\n      }\n\n      if (!list.length) {\n        return;\n      }\n\n      var $this = this;\n      $this.fullscreenLoading = true;\n      var $url = $this.$config.kds_data + \"data/tags/edit?keys=lane_400951053_1&namespace=hdmap\";\n      sysApi.postAsyncDatas({\n        url: $url,\n        data: list\n      }).then(res => {\n        $this.message(res.code, res.message);\n        $this.fullscreenLoading = false;\n        console.log(\"完成\");\n\n        if (res.code == \"0\") {\n          $this.taskId = null;\n          $this.$emit(\"refresh\");\n        }\n      });\n    },\n\n    // 刷新数据\n    refresh(param = {}) {\n      var page = param.page || this.pageNum;\n\n      if (param.id) {\n        this.taskId = param.id;\n      }\n\n      this.findLists(page);\n    },\n\n    // 刷新报表\n    refreshForm() {\n      this.$emit(\"refreshForm\", this.taskId);\n    },\n\n    // 分页查询\n    pageChange(page) {\n      this.pageTotleNum = 0;\n      this.activeLists[this.activeName] = [];\n\n      if (!this.taskId) {\n        return;\n      }\n\n      if (page) {\n        this.pageNum = page;\n      }\n\n      var $this = this;\n      var $url = $this.$config.kds_data + \"data/queryPage?keys=lane_400951053_1&namespace=hdmap\";\n      $this.fullscreenLoading = true;\n      sysApi.postAsyncDatas({\n        url: $url,\n        data: {\n          ops: [],\n          page: {\n            ops: [{\n              k: \"modelName\",\n              type: \"string\",\n              v: \"DIVIDER\",\n              op: \"in\"\n            }],\n            sort: [{\n              k: \"DIVIDER.SOURCE\",\n              order: \"desc\"\n            }, {\n              k: \"DIVIDER.R_LINE\",\n              order: \"desc\"\n            }],\n            page: {\n              pageNo: this.pageNum,\n              pageSize: 20\n            }\n          }\n        }\n      }).then(res => {\n        $this.fullscreenLoading = false;\n        $this.pageTotleNum = res.result.page.totalPages || 0;\n        $this.activeLists[$this.activeName] = res.result.data || [];\n      });\n    },\n\n    // 查询模型分类\n    findLists(value) {\n      if (!this.taskId) {\n        return;\n      }\n\n      var $this = this;\n      var $url = $this.$config.kds_data + \"data/queryPage?keys=lane_400951053_1&namespace=hdmap\";\n      $this.fullscreenLoading = true;\n      sysApi.postAsyncDatas({\n        url: $url,\n        data: {\n          ops: [],\n          page: {\n            pageSize: 10,\n            pageNo: 1\n          }\n        }\n      }).then(function (res) {\n        $this.fullscreenLoading = false;\n        $this.pageChange(value); // $this.activeName = $this.lists[0].value;\n        // $this.lists = [\n        //   {\n        //     name: \"路牌\",\n        //     value: \"1\",\n        //   },\n        //   {\n        //     name: \"灯杆\",\n        //     value: \"2\",\n        //   },\n        //   {\n        //     name: \"车道线\",\n        //     value: \"3\",\n        //   },\n        //   {\n        //     name: \"地面定位目标\",\n        //     value: \"4\",\n        //   },\n        // ];\n      });\n    },\n\n    message(type, message) {\n      if (type == \"0\") {\n        ElMessage.success({\n          message: message,\n          type: \"success\"\n        });\n      } else {\n        ElMessage.error(\"错误信息：\" + message);\n      }\n    }\n\n  },\n  components: {\n    contentSearch,\n    contentPage,\n    contentEdit\n  }\n};","map":{"version":3,"sources":["D:\\project\\batch-edit\\src\\components\\content.vue"],"names":[],"mappings":"AAyDA,OAAO,aAAP,MAA0B,kDAA1B;AACA,OAAO,WAAP,MAAwB,gDAAxB;AACA,OAAO,WAAP,MAAwB,gDAAxB;AACA,OAAO,KAAK,MAAZ,MAAwB,iBAAxB;AACA,SAAS,SAAT,QAA0B,cAA1B;AACA,eAAe;AACb,EAAA,IAAI,EAAE,QADO;;AAEb,EAAA,IAAI,GAAG;AACL,WAAO;AACL,MAAA,iBAAiB,EAAE,KADd;AAEL,MAAA,KAAK,EAAE,CACL;AACE,QAAA,IAAI,EAAE,IADR;AAEE,QAAA,KAAK,EAAE,UAFT;AAGE,QAAA,SAAS,EAAE;AAHb,OADK,EAML;AACE,QAAA,IAAI,EAAE,IADR;AAEE,QAAA,KAAK,EAAE,UAFT;AAGE,QAAA,SAAS,EAAE;AAHb,OANK,EAWL;AACE,QAAA,IAAI,EAAE,KADR;AAEE,QAAA,KAAK,EAAE,SAFT;AAGE,QAAA,SAAS,EAAE;AAHb,OAXK,EAgBL;AACE,QAAA,IAAI,EAAE,QADR;AAEE,QAAA,KAAK,EAAE,UAFT;AAGE,QAAA,SAAS,EAAE;AAHb,OAhBK,CAFF;AAwBL,MAAA,WAAW,EAAE,EAxBR;AAyBL,MAAA,KAAK,EAAE,EAzBF;AA0BL,MAAA,MAAM,EAAE,IA1BH;AA2BL,MAAA,YAAY,EAAE,CA3BT;AA4BL,MAAA,WAAW,EAAE,EA5BR;AA6BL,MAAA,OAAO,EAAE,CA7BJ;AA8BL,MAAA,UAAU,EAAE;AA9BP,KAAP;AAgCD,GAnCY;;AAoCb,EAAA,OAAO,GAAG;AACR,SAAK,OAAL,CAAa,CAAb;AACD,GAtCY;;AAuCb,EAAA,OAAO,EAAE;AACP;AACA,IAAA,WAAW,CAAC,GAAD,EAAM;AACf,UAAI,CAAC,KAAK,WAAL,CAAiB,GAAG,CAAC,KAAJ,CAAU,IAA3B,EAAiC,MAAtC,EAA8C;AAC5C,aAAK,UAAL,CAAgB,CAAhB;AACF;AACD,KANM;;AAOP,IAAA,UAAU,CAAC,KAAD,EAAQ;AAChB,MAAA,KAAK,CAAC,aAAN,CAAoB,SAApB,GAAgC,iBAAhC;AACD,KATM;;AAUP,IAAA,UAAU,CAAC,KAAD,E