loat: right; color: #8492a6; font-size: 13px\"\n          v-html=\"renderValue(item)\"\n        ></span>\n      </el-option>\n    </el-select>\n    <el-button\n      class=\"btn-cliam\"\n      type=\"primary\"\n      icon=\"el-icon-search\"\n      @click=\"cliam()\"\n      >领取</el-button\n    >\n    <el-button\n      class=\"btn-logout\"\n      type=\"primary\"\n      icon=\"el-icon-search\"\n      @click=\"logout()\"\n      >退出</el-button\n    >\n  </div>\n</template>\n\n<script>\nimport * as sysApi from \"../services/sys\";\nimport * as userApi from \"../services/jquery.oauth2-client\";\nexport default {\n  name: \"search\",\n  data() {\n    return {\n      searchTasks: [],\n      tasks: [],\n      value: \"\",\n    };\n  },\n  mounted() {\n    this.findTasks();\n  },\n  methods: {\n    changeTask(val) {\n      this.$emit(\"refresh\", val);\n    },\n    dataFilter(val) {\n      if (val) {\n        //val存在\n        this.searchTasks = this.tasks.filter((item) => {\n          var label = this.renderLabel(item);\n          var value = this.renderLabel(item);\n          if (\n            !!~value.indexOf(val) ||\n            !!~value.toUpperCase().indexOf(val.toUpperCase()) ||\n            !!~label.indexOf(val) ||\n            !!~label.toUpperCase().indexOf(val.toUpperCase())\n          ) {\n            return true;\n          }\n        });\n      } else {\n        //val为空时，还原数组\n        this.searchTasks = this.tasks;\n      }\n    },\n    findTasks() {\n      var $this = this;\n      var $url =\n        $this.$config.kts +\n        \"query/tasks?size=1000000&sort=createTime&order=desc&start=0\";\n      sysApi\n        .postAsyncDatas({\n          url: $url,\n          data: {\n            includeProcessVariables: true,\n            assignee: \"edit_lead1\",\n            excludeSubTasks: true,\n            processInstanceVariables: [\n              {\n                name: \"starter\",\n                value: \"%%\",\n                operation: \"like\",\n                type: \"string\",\n              },\n            ],\n          },\n        })\n        .then((res) => {\n          this.searchTasks = res.data;\n          this.tasks = res.data;\n        });\n    },\n    renderLabel(row) {\n      var variables = row.variables || [];\n      var taskName = variables.find((d) => {\n        return d.name == \"taskName\";\n      });\n      return taskName ? taskName.value : \" - \";\n    },\n    renderValue(row) {\n      var variables = row.variables || [];\n      var taskId = variables.find((d) => {\n        return d.name == \"taskId\";\n      });\n      return taskId ? taskId.value : \" - \";\n    },\n    cliam() {\n      var $this = this;\n      var $url =\n        $this.$config.kts +\n        \"query/tasks?size=1000000&sort=createTime&order=desc&start=0\";\n      sysApi\n        .postAsyncDatas({\n          url: $url,\n          data: {\n            includeProcessVariables: true,\n            assignee: \"edit_lead1\",\n            excludeSubTasks: true,\n            processInstanceVariables: [\n              {\n                name: \"starter\",\n                value: \"%%\",\n                operation: \"like\",\n                type: \"string\",\n              },\n            ],\n          },\n        })\n        .then((res) => {\n          console.log(res);\n          this.findTasks();\n        });\n    },\n    save() {\n      console.log(\"保存\");\n    },\n    logout() {\n      userApi.oAuthLoginPage();\n    },\n  },\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped lang=\"scss\">\n.search {\n  width: calc(100% - 20px);\n  height: 40px;\n  float: left;\n  padding: 5px 10px;\n}\n.search .el-select {\n  float: left;\n}\n.search button {\n  float: left;\n  padding: 0px 15px;\n}\n.search button.btn-cliam {\n  margin-left: 30px;\n}\n.search button.btn-logout {\n  float: right;\n}\n</style>\n"],"sourceRoot":""},"metadata":{},"sourceType":"module"}