{"ast":null,"code":"import * as sysApi from \"../services/sys\";\nimport * as userApi from \"../services/jquery.oauth2-client\";\nexport default {\n  name: \"search\",\n\n  data() {\n    return {\n      catchTasks: {},\n      searchTasks: [],\n      tasks: [],\n      value: \"\"\n    };\n  },\n\n  // 初始化\n  mounted() {\n    this.findTasks();\n  },\n\n  methods: {\n    // 每次切换任务时执行\n    changeTask(val) {\n      var featureGroupId = this.catchTasks[val].featureGroupId;\n      this.$emit(\"refresh\", val, featureGroupId);\n    },\n\n    // 根据用户的任务列表进行筛选\n    dataFilter(val) {\n      if (val) {\n        //val存在\n        this.searchTasks = this.tasks.filter(item => {\n          var label = this.renderLabel(item);\n          var value = this.renderValue(item);\n\n          if (!!~value.indexOf(val) || !!~value.toUpperCase().indexOf(val.toUpperCase()) || !!~label.indexOf(val) || !!~label.toUpperCase().indexOf(val.toUpperCase())) {\n            return true;\n          }\n        });\n      } else {\n        //val为空时，还原数组\n        this.searchTasks = this.tasks;\n      }\n    },\n\n    // 第一次查询任务信息--部分信息（通过用户）\n    findTasks() {\n      var $this = this;\n      var $url = $this.$config.kts + \"runtime/tasks?includeProcessVariables=true&size=100&sort=createTime&order=desc&assignee=\" + $this.$user.name;\n      $this.catchTasks = {};\n      $this.searchTasks = [];\n      $this.tasks = [];\n      $this.value = \"\";\n      sysApi.getAsyncDatas({\n        url: $url,\n        data: {}\n      }).then(res => {\n        var datas = res.data || [];\n        datas.forEach(d => {\n          var taskId = d.variables.find(s => {\n            return s.name == \"taskId\";\n          });\n\n          if (taskId) {\n            $this.catchTasks[taskId.value] = d;\n          }\n        });\n        $this.findTaskLists();\n      });\n    },\n\n    // 第二次查询任务信息--详细信息（通过id）\n    findTaskLists() {\n      var $this = this;\n      var formData = new FormData();\n      var ids = [];\n\n      for (var taskId in $this.catchTasks) {\n        ids.push(taskId);\n      }\n\n      if (!ids.length) {\n        return;\n      }\n\n      formData.append(\"ids\", ids.join(\",\"));\n      var $url = $this.$config.kts + \"task/findByIds\";\n      sysApi.postAsyncDatas({\n        url: $url,\n        data: formData\n      }).then(res => {\n        var results = res.result || {};\n\n        for (var id in results) {\n          var featureGroups = results[id].featureGroups || [];\n          var taskDefinitionKey = $this.catchTasks[id].taskDefinitionKey;\n          var feature = featureGroups.find(d => {\n            return d.linkId == taskDefinitionKey;\n          });\n\n          if (feature) {\n            $this.catchTasks[id][\"featureGroupId\"] = feature.featureGroupId;\n            results[id][\"featureGroupId\"] = feature.featureGroupId;\n            results[id][\"taskId\"] = id;\n            results[id][\"taskName\"] = results[id].name;\n            $this.searchTasks.push(results[id]);\n            $this.tasks.push(results[id]);\n          }\n        }\n      });\n    },\n\n    renderLabel(row) {\n      return row.taskName || \" - \";\n    },\n\n    renderValue(row) {\n      return row.taskId || \" - \";\n    },\n\n    // 领取任务\n    cliam() {\n      var $this = this;\n      var $url = $this.$config.kts + \"task/claim?user=\" + $this.$user.name;\n      sysApi.getAsyncDatas({\n        url: $url,\n        data: {}\n      }).then(res => {\n        if (res.code == \"0\" && res.result.length) {\n          $this.$notify({\n            title: \"成功\",\n            message: res.message,\n            type: \"success\"\n          });\n          $this.findTasks();\n        } else {\n          $this.$notify({\n            title: \"警告\",\n            message: res.message,\n            type: \"warning\"\n          });\n        }\n      });\n    },\n\n    save() {// console.log(\"保存\");\n    },\n\n    // 用户登出\n    logout() {\n      userApi.oauth2Client(null, this.$config.kd_auth_server).signout();\n    }\n\n  }\n};","map":{"version":3,"sources":["D:\\web_editor_hd\\src\\main\\webapp\\batch-edit\\src\\components\\search.vue"],"names":[],"mappings":"AA2CA,OAAO,KAAK,MAAZ,MAAwB,iBAAxB;AACA,OAAO,KAAK,OAAZ,MAAyB,kCAAzB;AACA,eAAe;AACb,EAAA,IAAI,EAAE,QADO;;AAEb,EAAA,IAAI,GAAG;AACL,WAAO;AACL,MAAA,UAAU,EAAE,EADP;AAEL,MAAA,WAAW,EAAE,EAFR;AAGL,MAAA,KAAK,EAAE,EAHF;AAIL,MAAA,KAAK,EAAE;AAJF,KAAP;AAMD,GATY;;AAUb;AACA,EAAA,OAAO,GAAG;AACR,SAAK,SAAL;AACD,GAbY;;AAcb,EAAA,OAAO,EAAE;AACP;AACA,IAAA,UAAU,CAAC,GAAD,EAAM;AACd,UAAI,cAAa,GAAI,KAAK,UAAL,CAAgB,GAAhB,EAAqB,cAA1C;AACA,WAAK,KAAL,CAAW,SAAX,EAAsB,GAAtB,EAA2B,cAA3B;AACD,KALM;;AAMP;AACA,IAAA,UAAU,CAAC,GAAD,EAAM;AACd,UAAI,GAAJ,EAAS;AACP;AACA,aAAK,WAAL,GAAmB,KAAK,KAAL,CAAW,MAAX,CAAmB,IAAD,IAAU;AAC7C,cAAI,KAAI,GAAI,KAAK,WAAL,CAAiB,IAAjB,CAAZ;AACA,cAAI,KAAI,GAAI,KAAK,WAAL,CAAiB,IAAjB,CAAZ;;AACA,cACE,CAAC,CAAC,CAAC,KAAK,CAAC,OAAN,CAAc,GAAd,CAAH,IACA,CAAC,CAAC,CAAC,KAAK,CAAC,WAAN,GAAoB,OAApB,CAA4B,GAAG,CAAC,WAAJ,EAA5B,CADH,IAEA,CAAC,CAAC,CAAC,KAAK,CAAC,OAAN,CAAc,GAAd,CAFH,IAGA,CAAC,CAAC,CAAC,KAAK,CAAC,WAAN,GAAoB,OAApB,CAA4B,GAAG,CAAC,WAAJ,EAA5B,CAJL,EAKE;AACA,mBAAO,IAAP;AACF;AACD,SAXkB,CAAnB;AAYF,OAdA,MAcO;AACL;AACA,aAAK,WAAL,GAAmB,KAAK,KAAxB;AACF;AACD,KA1BM;;AA2BP;AACA,IAAA,SAAS,GAAG;AACV,UAAI,KAAI,GAAI,IAAZ;AACA,UAAI,IAAG,GACL,KAAK,CAAC,OAAN,CAAc,GAAd,GACA,0FADA,GAEA,KAAK,CAAC,KAAN,CAAY,IAHd;AAIA,MAAA,KAAK,CAAC,UAAN,GAAmB,EAAnB;AACA,MAAA,KAAK,CAAC,WAAN,GAAoB,EAApB;AACA,MAAA,KAAK,CAAC,KAAN,GAAc,EAAd;AACA,MAAA,KAAK,CAAC,KAAN,GAAc,EAAd;AACA,MAAA,MAAK,CACF,aADH,CACiB;AACb,QAAA,GAAG,EAAE,IADQ;AAEb,QAAA,IAAI,EAAE;AAFO,OADjB,EAKG,IALH,CAKS,GAAD,IAAS;AACb,YAAI,KAAI,GAAI,GAAG,CAAC,IAAJ,IAAY,EAAxB;AACA,QAAA,KAAK,CAAC,OAAN,CAAe,CAAD,IAAO;AACnB,cAAI,MAAK,GAAI,CAAC,CAAC,SAAF,CAAY,IAAZ,CAAkB,CAAD,IAAO;AACnC,mBAAO,CAAC,CAAC,IAAF,IAAU,QAAjB;AACD,WAFY,CAAb;;AAGA,cAAI,MAAJ,EAAY;AACV,YAAA,KAAK,CAAC,UAAN,CAAiB,MAAM,CAAC,KAAxB,IAAiC,CAAjC;AACF;AACD,SAPD;AAQA,QAAA,KAAK,CAAC,aAAN;AACD,OAhBH;AAiBD,KAvDM;;AAwDP;AACA,IAAA,aAAa,GAAG;AACd,UAAI,KAAI,GAA